{
  "id": null,
  "uid": "mono-pipeline-observability",
  "title": "Mono Banking Pipeline",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Pipeline Stage P95 Latency",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1.5 },
              { "color": "red", "value": 2 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (stage, le) (rate(taxpoynt_mono_pipeline_stage_seconds_bucket[5m])))",
          "legendFormat": "{{stage}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 2,
      "title": "Sync Failure Burndown",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "sum by (stage, reason) (increase(taxpoynt_mono_pipeline_errors_total[15m]))",
          "legendFormat": "{{stage}} - {{reason}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 3,
      "title": "Zero Transaction Runs",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "increase(taxpoynt_mono_pipeline_zero_transactions_total[1h])",
          "legendFormat": "{{provider}}",
          "refId": "A"
        }
      ]
    }
  ],
  "templating": {
    "list": []
  },
  "annotations": {
    "list": [
      {
        "type": "dashboard",
        "name": "Annotations & Alerts",
        "enabled": true,
        "hide": false,
        "builtIn": 1,
        "iconColor": "rgba(0, 211, 255, 1)",
        "typeLogoUrl": "",
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        }
      }
    ]
  }
}
